{
  "name": "bjeco",
  "version": "0.2.0",
  "private": true,
  "description": "https://bje.co",
  "repository": "https://github.com/brianespinosa/bjeco.git",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test:bjeco": "nx run bjeco:test",
    "test:blocks": "nx run blocks:test",
    "e2e:bjeco": "nx e2e bjeco-e2e",
    "e2e:blocks": "nx e2e blocks-e2e",
    "storybook:blocks": "nx run blocks:storybook",
    "nx-latest": "nx migrate latest",
    "nx-migrate": "yarn install && nx migrate --run-migrations"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,sass,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,html}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@bjeco/prettier-config",
  "stylelint": {
    "extends": "@bjeco/stylelint-config"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "nth-check": "2.0.1"
  },
  "dependencies": {
    "antd": "4.6.3",
    "ariakit": "2.0.0-next.24",
    "clsx": "1.1.1",
    "core-js": "^3.6.5",
    "fathom-client": "3.4.1",
    "framer-motion": "6.3.1",
    "next": "12.1.5",
    "normalize.css": "8.0.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@bjeco/prettier-config": "latest",
    "@bjeco/stylelint-config": "latest",
    "@nrwl/cli": "14.0.3",
    "@nrwl/cypress": "14.0.3",
    "@nrwl/eslint-plugin-nx": "14.0.3",
    "@nrwl/jest": "14.0.3",
    "@nrwl/linter": "14.0.3",
    "@nrwl/next": "14.0.3",
    "@nrwl/nx-cloud": "13.3.1",
    "@nrwl/react": "14.0.3",
    "@nrwl/storybook": "14.0.3",
    "@nrwl/web": "14.0.3",
    "@nrwl/workspace": "14.0.3",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-essentials": "~6.4.12",
    "@storybook/builder-webpack5": "~6.4.12",
    "@storybook/core-server": "~6.4.12",
    "@storybook/manager-webpack5": "~6.4.12",
    "@storybook/react": "~6.4.12",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "13.1.1",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.1.0",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-junit": "11.1.0",
    "lint-staged": "12.4.0",
    "nx": "14.0.3",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "require-context.macro": "1.2.2",
    "sass": "1.50.1",
    "storybook-dark-mode": "1.0.9",
    "stylelint": "14.7.1",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "~4.6.2",
    "url-loader": "^3.0.0"
  },
  "packageManager": "yarn@3.2.0"
}
