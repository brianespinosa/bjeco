  "scripts": {
    "start": "nx serve",
    "lint": "nx run-many --all --target=lint",
    "test": "nx run-many --all --target=test",
    "e2e": "nx run-many --all --target=e2e",
    "build": "nx build",
    "publish": "nx publish blocks",
    "build:storybook": "nx run blocks:build-storybook",
    "test:bjeco": "nx run bjeco:test",
    "test:blocks": "nx run blocks:test",
    "e2e:bjeco": "nx e2e bjeco-e2e",
    "e2e:blocks": "nx e2e blocks-e2e",
    "storybook:blocks": "nx run blocks:storybook",
    "nx-latest": "nx migrate latest",
    "nx-migrate": "yarn install && nx migrate --run-migrations",
    "postinstall": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
